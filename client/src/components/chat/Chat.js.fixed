          open={incomingCallOpen}
          caller={incomingCallData?.caller}
          onAccept={callAcceptHandler}
          onReject={callRejectHandler}
          onClose={() => setIncomingCallOpen(false)}
        />
      </Box>
  );
};

export default Chat; 


